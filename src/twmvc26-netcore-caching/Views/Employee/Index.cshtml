@model IEnumerable<twmvc26_netcore_caching.Models.Employee>

@{

    ViewData["Title"] = "IMemoryCache Sample";
}

<h2>@ViewData["Title"]</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EmpNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmpName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EmpNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmpName)
            </td>
        </tr>
    }
</table>

<div>
    @{
        <input type="text" name="txtName" value="@(ViewBag.Status)" style="width:400px;" />
    }
</div>